name: A workflow with multiple jobs

on:
  push:
  branches: ["main"]

jobs:
  first-job:
    runs-on: windows-latest

    steps:
      - name: Checkout
        uses: actions/checkout@v3

  second-job:
  needs: first-job
    runs-on: windows-latest

    steps:
      - name: Echo something
        run: echo "Oumar Yanni"
